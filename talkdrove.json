{
    "bot-name": "Zokou-MD English",
    "description": "Simple whatsapp bot for groups and personal chats ; Master script",
    "logo": "https://raw.githubusercontent.com/Luffy2ndAccount/zokou-english-v2/refs/heads/main/Zokou.jpg",
    "keywords": [
        "bot , Zokou"
    ],
    "documentation-link": "https://zokou-web.onrender.com",
    "owner-verification": "charpesstearviky@gmail.com",
    "env": {
        "PREFIX": {.
            "description": "choose your bot prefix",
            "value": "#",
            "required": true
        },
        "AUTO_READ_STATUS": {yes
            "description": "Your contact status will be read automatically (type yes to activate or no to deactivate; don't write in capital letters)",
            "value": "yes",
            "required": false
        },
        "AUTO_DOWNLOAD_STATUS": {yes
            "description": "Your contact status will be downloaded automatically and sent to you (type yes to activate or no to deactivate; don't write in capital letters)",
            "value": "yes",
            "required": false
        },
        "PM_PERMIT": {no
            "description": "Other users can't use your bot in private messages (type yes to activate or no to deactivate; don't write in capital letters)",
            "value": "no",
            "required": false
        },
        "BOT_NAME": {zokou
            "description": "put a name for your bot",
            "value": "Zokou-MD",
            "required": false
        },
        "BOT_MENU_LINKS": {luffy
            "description": "add one or many links for your bot menu; e.g., url1,url2,url3..... || Or a theme name",
            "value": "LUFFY",
            "required": false
        },
        "PUBLIC_MODE": {false
            "description": "type yes to put your bot in public mode or no to put it in private mode",
            "value": "yes",
            "required": false
        },
        "SESSION_ID": {Zokou-MD-WHATSAPP-BOT;;;=>
            "description": "put your session ID, the code you receive after scanning the QR code",
            "value": "",
            "required": true
        },
        "OWNER_NAME": {alte+
            "description": "Your Name",
            "required": false,
            "value": "alte+"
        },
        "NUMERO_OWNER": {50944764875
            "description": "Your number, don't put '+' and include your area code (example for Togo '228')",
            "required": false,
            "value": "50944764875"
        },
        "WARN_COUNT": {3
            "description": "this is the limit of warnings for warning commands",
            "required": false,
            "value": "3"
        },
        "STARTING_BOT_MESSAGE": {true
            "description": "if you don't want a starting-bot-message put no, otherwise put yes",
            "required": true,
            "value": "yes"
        },
        "PRESENCE": {3
            "description": "Mark your presence: 1 to indicate that you are online even if you are not. 2 to indicate that you are currently typing a message. 3 to indicate that you are currently recording an audio. Or leave the field blank to indicate your real state.",
            "value": "3",
            "required": false
        },
        "PM_CHATBOT": {no
            "description": "Put yes if you want the bot to automatically respond to messages in private",
            "required": false,
            "value": "no"
        },
        "ANTI_VIEW_ONCE": {yes
            "description": "Put yes if you want to receive all view once content in private messages",
            "required": false,
            "value": "yes"
        },
        "ANTI_COMMAND_SPAM": {no
            "description": "activate a cooldown of 10 seconds for every user after a command to avoid spamming unless owner and mods",
            "required": false,
            "value": "no"
        },
        "ANTI_DELETE_MESSAGE": {yes
            "description": "Put yes if you want messages deleted by other users to be sent to you in private messages",
            "required": false,
            "value": "yes"
        },
        "AUTO_REACT_MESSAGE": {yes
            "description": "Put yes if you want messages to be automatically reacted to",
            "required": false,
            "value": "yes"
        }
    }
}
